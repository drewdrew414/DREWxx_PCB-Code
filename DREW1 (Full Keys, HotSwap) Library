void GPIO_Init(void) {
__HAL_RCC_GPIOA_CLK_ENABLE(); //Turn On GPIOA Pins
    __HAL_RCC_GPIOB_CLK_ENABLE(); //Turn On GPIOB Pins
        __HAL_RCC_GPIOC_CLK_ENABLE(); //Turn On GPIOC Pins
            __HAL_RCC_GPIOD_CLK_ENABLE(); //Trun On GPIOD Pins
                __HAL_RCC_GPIOE_CLK_ENABLE(); //Trun On GPIOE Pins




GPIO_InitTypeDef GPIO InitStruct_B = {0};
    GPIO_InStruct_B.Pin = GPIO_PIN_○ | GPIO_PIN_○ | ••• ;
//Setting Pins
        GPIO_InitStruct_B.Mode = GPIO_MODE_INPUT;
//Setting Modes
            GPIO_InitStruct_B.Pull = GPIO_PULLUP;
//Setting Resistor Pull Up or Down
                GPIO_InitStruct_B.Speed = GPIO_SPEED_FREQ_LOW;
//Setting Speed
                    HAL_GPIO_Init(GPIOB,  &GPIO_InitStruct_B);
//Reset GPIOB PORT




