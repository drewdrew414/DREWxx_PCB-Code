#include "stm32f4xx.h" //stm32f407vgt6 MCU

#define COLS //Amount of COLS
#define ROWS //Amount of ROWS

const uint16_t rowPins[ROWS] = {GPIO_PIN_
const uint16_t colPins[COLS] = {GPIO_PIN_

const char keyMap[ROWS][COLS] = {
{'ESC_key','F1','F2','F3','F4'

}
void MX_GPIO_Init(void) {
    GPIO_InitTypeDef GPIO_InitStruct = {0};
//Reset GPIO Pins
    __HAL_RCC_GPIOA_CLK_ENABLE();
//Active GPIOA
    __HAL_RCC_GPIOB_CLK_ENABLE();
//Active GPIOB

//Input Pins code
    GPIO_InitStruct.Pin = GPIO_PIN_;
//Specify Pins
    GPIO_InitStruct.Mode = GPIO_MODE_INPUT;
//Making Pins Input
    GPIO_InitStruct.Pull = GPIO_PULLUP;
//Active PullUp Resistor
    HAL_GPIO_Init(GPIOB, &GPIO_InitStruct);
//Reset Pins


//Output Pins Code
    GPIO_InitStruct.Pin = GPIO_PIN_;
//Specify Pins
    GPIO_InitStruct.Mode = GPIO_MODE_OUTPUT_PP;
//Making Pins Push Pull Output
    GPIO_InitStruct.Pull = GPIO_NOPULL;
//No Pullup or Pulldown Resistor
    GPIO_InitStruct.Speed = GPIO_SPEED_FREQ_LOW;
//Communication Speed
    HAL_GPIO_Init(GPIOA, &GPIO_InitStruct);
//Reset Pins
}



