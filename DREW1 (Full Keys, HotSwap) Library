void GPIO_Init(void) {
__HAL_RCC_GPIOA_CLK_ENABLE(); //Turn On GPIOA Pins
    __HAL_RCC_GPIOC_CLK_ENABLE(); //Turn On GPIOC Pins
        __HAL_RCC_GPIOD_CLK_ENABLE(); //Trun On GPIOD Pins
            __HAL_RCC_GPIOE_CLK_ENABLE(); //Trun On GPIOE Pins



//About PortD

GPIO_InitTypeDef GPIO InitStruct_D = {0};
    GPIO_InStruct_D.Pin = GPIO_PIN_15 | GPIO_PIN_14 | GPIO_PIN_13 | GPIO_PIN_12 | GPIO_PIN_11 | GPIO_PIN_10;
//Setting Pins
        GPIO_InitStruct_D.Mode = GPIO_MODE_INPUT;
//Setting Modes
            GPIO_InitStruct_D.Pull = GPIO_PULLUP;
//Setting Resistor Pull Up or Down
                GPIO_InitStruct_D.Speed = GPIO_SPEED_FREQ_LOW;
//Setting Speed
                    HAL_GPIO_Init(GPIOD,  &GPIO_InitStruct_D);
//Reset GPIOD PORT


//About PortC


GPIO_InitTypeDef GPIO InitStruct_C = {0};
    GPIO_InStruct_C.Pin = GPIO_PIN_○ | GPIO_PIN_○ | ••• ;
//Setting Pins
        GPIO_InitStruct_C.Mode = GPIO_MODE_OUTPUT_PP;
//Setting Modes
            GPIO_InitStruct_C.Pull = GPIO_NOPULL;
//Setting Resistor Pull Up or Down
                GPIO_InitStruct_C.Speed = GPIO_SPEED_FREQ_LOW;
//Setting Speed
                    HAL_GPIO_Init(GPIOC,  &GPIO_InitStruct_C);
//Reset GPIOC PORT


//About Port E

GPIO_InitTypeDef GPIO InitStruct_E = {0};
    GPIO_InStruct_E.Pin = GPIO_PIN_○ | GPIO_PIN_○ | •••;
//Setting Pins
        GPIO_InitStruct_D.Mode = GPIO_MODE_OUTPUT_PP;
//Setting Modes
            GPIO_InitStruct_D.Pull = GPIO_NOPULL;
//Setting Resistor Pull Up or Down
                GPIO_InitStruct_D.Speed = GPIO_SPEED_FREQ_LOW;
//Setting Speed
                    HAL_GPIO_Init(GPIOD,  &GPIO_InitStruct_D);
//Reset GPIOD PORT


//About PortA

